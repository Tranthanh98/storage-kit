{
  "name": "@storage-kit/client",
  "version": "2.0.0",
  "description": "Framework-agnostic TypeScript client SDK for Storage Kit - easy file upload, delete, and signed URL generation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "storage-kit",
    "client",
    "sdk",
    "s3",
    "file-upload",
    "presigned-url",
    "react",
    "react-native",
    "vue",
    "angular",
    "typescript"
  ],
  "author": "Tranthanh98",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tranthanh98/storage-kit.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/Tranthanh98/storage-kit/issues"
  },
  "homepage": "https://tranthanh98.github.io/storage-kit/",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
