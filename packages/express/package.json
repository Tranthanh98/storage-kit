{
  "name": "@storage-kit/express",
  "version": "2.0.0",
  "description": "Express.js adapter for Storage Kit - plug-and-play storage HTTP endpoints",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "storage-kit",
    "express",
    "s3",
    "file-upload",
    "multipart",
    "middleware",
    "router"
  ],
  "author": "Tranthanh98",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tranthanh98/storage-kit.git",
    "directory": "packages/express"
  },
  "bugs": {
    "url": "https://github.com/Tranthanh98/storage-kit/issues"
  },
  "homepage": "https://tranthanh98.github.io/storage-kit/",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@storage-kit/core": "workspace:*",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/swagger-ui-express": "^4.1.6",
    "express": "^4.18.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  }
}
